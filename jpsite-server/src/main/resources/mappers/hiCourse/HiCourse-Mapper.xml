<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mty.jpsite.server.dao.hiCourse.HiCourseDao">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mty.jpsite.server.entity.hiCourse.HiCourse">
        <id column="id" property="id" />
        <result column="course" property="course" />
        <result column="teacher" property="teacher" />
        <result column="class_hour_start" property="classHourStart" />
        <result column="class_hour_down" property="classHourDown" />
        <result column="classroom" property="classroom" />
        <result column="create_at" property="createAt" />
        <result column="update_at" property="updateAt" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, course, teacher, class_hour_start, class_hour_down, classroom, create_at, update_at
    </sql>

</mapper>
